{
    "name": "stackmash/stackmash-laravel",
    "description": "",
    "keywords": [
        "stackmash",
        "stackmash-laravel"
    ],
    "homepage": "https://github.com/stackmash/stackmash-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stackmash",
            "email": "support@stackmash.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stackmash\\StackmashLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stackmash\\StackmashLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stackmash\\StackmashLaravel\\StackmashLaravelServiceProvider"
            ],
            "aliases": {
                "StackmashLaravel": "Stackmash\\StackmashLaravel\\StackmashLaravelFacade"
            }
        }
    }
}
